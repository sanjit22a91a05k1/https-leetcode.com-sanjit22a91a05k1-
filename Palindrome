bool isPalindrome(int x){
    int t=x;
    long rev=0;
    while(x!=0)
    {
        int r=x%10;
        rev=rev*10+r;
        x=x/10;
    }
    if(rev>=0)
    {
    if(rev==t)
    {
        return 1;
    }
    else 
    {
        return 0;
    }
    }
     else 
    {
        return 0;
    }
    
}
